                <fields>
                    <field tag="1" name="condition" optional="false" privacy="false">
                        <dataType>
                            <kind>STRUCT</kind>
                            <ref>com.github.dapeng.basic.api.counter.domain.DataPoint</ref>
                        </dataType>
                        <doc></doc>
                    </field>
                    <field tag="2" name="beginTimeStamp" optional="false" privacy="false">
